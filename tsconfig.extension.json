{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "commonjs",
    "target": "es6",
    "outDir": "./dist/webpack_bundles/js/",
    "lib": [
      "es6",
      "es2018",
      "ES2019",
      "ES2020",
    ],
    "sourceMap": true,
    "rootDir": "api/static",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitThis": false
  },
  "exclude": [
    "node_modules",
    ".vscode-test",
    ".vscode test",
    "dist"
  ]
}